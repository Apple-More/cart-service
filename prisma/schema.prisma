generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Cart_Items {
  cart_item_id String @id @default(auto()) @map("_id") @db.ObjectId
  quantity     Int
  product_variant_id String 
  customer_id String
}
